{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "cma",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "instruction"
      }
    },
    "instruction": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "mnemonic",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "load"
              },
              {
                "type": "STRING",
                "value": "loadc"
              }
            ]
          }
        },
        {
          "type": "FIELD",
          "name": "operand",
          "content": {
            "type": "SYMBOL",
            "name": "number"
          }
        }
      ]
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}